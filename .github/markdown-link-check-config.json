{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/.*/issues/\\d+"
    },
    {
      "pattern": "^https://github.com/.*/pull/\\d+"
    },
    {
      "pattern": "^https://github.com/.*/commit/"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "^https://stackoverflow\\.com/"
    },
    {
      "pattern": "^https://[a-z]+\\.stackexchange\\.com/"
    },
    {
      "pattern": "^https://www\\.gamedev\\.net/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com/"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
