{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "src",
          "files": [
            "KeenEyes.Core/**/*.csproj",
            "KeenEyes.Generators.Attributes/**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net10.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appTitle": "KeenEyes ECS",
      "_appName": "KeenEyes",
      "_appFooter": "KeenEyes ECS Framework",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "fileMetadata": {},
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
